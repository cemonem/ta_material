device PIC18F4620
hwtool SIM
set warningmessagebreakoptions.W0223_ADC_UNDERFLOW ignore
set warningmessagebreakoptions.W0222_ADC_OVERFLOW ignore
set oscillator.frequency 10
set oscillator.frequencyunit Mega
program "${cof_file_loc}"

reset MCLR

write pin RB0 high
write pin RB1 high
write pin RB2 high
write pin RB3 high

break *${init_complete}
run

wait
delete
break *${sec_passed}
continue

wait
echo sec_result: stopwatch should be 1.0Â±0.05 seconds
stopwatch

quit
